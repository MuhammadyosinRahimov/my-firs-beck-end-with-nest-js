{
    "version":2,
    "build":[
        {
            "src":"src/main.ts",
            "use":"@vercel/node"
        }
    ],
    "route":[
        {
            "src":"^/(.*)$",
            "dest":"src/main.ts",
            "method":[
                "GET",
                "DELET",
                "POST",
                "PUT",
                "PATCH"

            ]

        }
    ]
}